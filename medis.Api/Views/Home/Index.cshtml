<!doctype html>
<html lang="en" ng-app="medisApp">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Angular app</title>
    <link rel="stylesheet" type="text/css" href="/content/css/bootstrap/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="/content/css/bootstrap/bootstrap-theme.css" />
    <link rel="stylesheet" type="text/css" href="/content/css/jquery-ui.min.css" />
    <link rel="stylesheet" type="text/css" href="/content/css/animate.css" />
    <link rel="stylesheet" type="text/css" href="/content/css/style.css" />
    <script type="text/javascript" src="/content/scripts/jquery.min.js"></script>
    <script type="text/javascript" src="/content/scripts/jquery-ui.js"></script>
    <script type="text/javascript" src="/content/scripts/angular.min.js"></script>
    <base href="/"/>
</head>

<body ng-controller="MainCtrl">
    <nav class="navbar navbar-fixed-top navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">Project name</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li ng-class="activeNavClass('/videos')">
                        <a href="/videos">Videos</a>
                    </li>
                    <li dropdown>
                        <a href class="dropdown-toggle" dropdown-toggle>Images (not implemented) <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#">Action</a></li>
                            <li><a href="#">Another action</a></li>
                            <li><a href="#">Something else here</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Separated link</a></li>
                        </ul>
                    </li>
                    <li ng-class="activeNavClass('/music')">
                        <a href="/music">Music (not implemented)</a>
                    </li>
                    <li dropdown ng-class="activeNavClass('/admin')">
                        <a href class="dropdown-toggle" dropdown-toggle>Admin <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li ng-class="activeNavClass('/admin/videos')">
                                <a href="/admin/videos">Videos</a>
                            </li>
                            <li ng-class="activeNavClass('/admin/images')">
                                <a href="/admin/images">Images</a>
                            </li>
                            <li ng-class="activeNavClass('/admin/audios')">
                                <a href="/admin/audios">Audios</a>
                            </li>
                            <li ng-class="activeNavClass('/admin/test-media')">
                                <a href="/admin/test-media">Test Media</a>
                            </li>
                        </ul>
                    </li>
                    <li ng-class="{ 'active': activeNavClass('/test-media') }">
                        <a href="/test-media">Test Media</a>
                    </li>
                </ul>
            </div>
            <!-- /.nav-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    <!-- /.navbar -->
    <div class="container">
        <div ng-cloak>
            <alert type="{{ alertType }}" ng-show="showTempMsg" class="text-center slide-fade" close="closeTempMsg()">
                <div ng-bind-html="tempMsg"></div>
            </alert>
        </div>
        <!-- <div class="row">
            <input type="button" class="btn btn-info" ng-click="setErrorMsg('test')" value="testError" />
            <input type="button" class="btn btn-info" ng-click="setWarningMsg('test')" value="testWarning" />
            <input type="button" class="btn btn-info" ng-click="setSuccessMsg('test')" value="testSuccess" />
        </div> -->
        <div class="row row-offcanvas row-offcanvas-right">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <!--<p class="pull-right visible-xs">
                    <input type="submit" class="btn btn-primary btn-xs" data-toggle="offcanvas">Toggle nav</button>
                </p>-->
                <div class="row" ng-view></div>
            </div>

        </div>
        <hr>
        <footer>
            <p>&copy; Company 2015</p>
        </footer>
    </div>
    <!--/.container-->
    <!-- vendor libs -->
    <script type="text/javascript" src="/content/scripts/angular-route.min.js"></script>
    <script type="text/javascript" src="/content/scripts/angular-sanitize.min.js"></script>
    <script type="text/javascript" src="/content/scripts/angular-animate.min.js"></script>
    <script type="text/javascript" src="/content/scripts/ng-file-upload-shim.min.js"></script>
    <script type="text/javascript" src="/content/scripts/ng-file-upload.min.js"></script>
    <script type="text/javascript" src="/content/scripts/ui-bootstrap-tpls-0.13.0.min.js"></script>
    <script type="text/javascript" src="/content/scripts/TweenMax.min.js"></script>
    <script type="text/javascript" src="/content/scripts/angular-wizard.min.js"></script>
    <script type="text/javascript" src="/content/scripts/underscore-min.js"></script>
    <!-- modules -->
    <script type="text/javascript" src="/app/app.js"></script>
    <script type="text/javascript" src="/app/modules/ctrl-module.js"></script>
    <script type="text/javascript" src="/app/modules/svc-module.js"></script>
    <script type="text/javascript" src="/app/modules/filters-module.js"></script>
    <script type="text/javascript" src="/app/modules/directives-module.js"></script>
    <!-- controllers -->
    <script type="text/javascript" src="/app/controllers/main-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/videos/video-home-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/videos/video-files-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/admin/admin-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/admin/admin-video-add-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/admin/admin-video-edit-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/shared/modal-ctrl.js"></script>
    <script type="text/javascript" src="/app/controllers/test-media-ctrl.js"></script>
    <!-- services -->
    <script type="text/javascript" src="/app/services/admin-svc.js"></script>
    <script type="text/javascript" src="/app/services/video-svc.js"></script>
    <script type="text/javascript" src="/app/services/message-svc.js"></script>
    <script type="text/javascript" src="/app/services/custom-args-svc.js"></script>
    <!-- filters -->
    <script type="text/javascript" src="/app/filters/allow-null.js"></script>
    <script type="text/javascript" src="/app/filters/trustHtml.js"></script>
    <!--<script type="text/javascript" src="/app/directives/alert-msg.js"></script>-->
    <!-- animations -->
    <script type="text/javascript" src="/app/animations/slide-fade.js"></script>
    <script type="text/javascript" src="/app/animations/fade-in-out.js"></script>

    <!-- directives-->
    <script type="text/javascript" src="/app/directives/alert-msg.js"></script>
    <script type="text/javascript" src="/app/directives/views.js"></script>
    <script type="text/javascript" src="/app/directives/sw-accordion.js"></script>
    <script type="text/javascript" src="/app/directives/jqTests.js"></script>
</body>

</html>
